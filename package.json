{
  "name": "ngcatalyst",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "copy-license": "cp ./LICENSE ./dist/ngcatalyst",
    "copy-readme": "cp ./README.md ./dist/ngcatalyst",
    "copy-files": "npm run copy-license && npm run copy-readme",
    "package_styles": "ng-packagr -p ./projects/ngcatalyst/ng-package.json && cp -r ./projects/ngcatalyst/src/lib/styles.css ./dist/ngcatalyst/lib/styles.css",
    "build_lib": "ng build ngcatalyst && npm run package_styles",
    "npm_pack": "cd dist/ngcatalyst && npm pack",
    "package": "npm run build_lib && npm run copy-files && npm run npm_pack",
    "publish-script": "npm run package && cd dist/ngcatalyst && npm publish"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@fortawesome/angular-fontawesome": "^0.5.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.22",
    "@fortawesome/free-solid-svg-icons": "^5.10.2",
    "core-js": "^2.5.4",
    "d3": "^5.9.7",
    "font-awesome": "^4.7.0",
    "lodash": "^4.17.15",
    "luxon": "^1.17.1",
    "rxjs": "~6.3.3",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.12.4",
    "@angular-devkit/build-ng-packagr": "~0.12.0",
    "@angular/cli": "~7.2.2",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^8.10.51",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.6",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.2.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.36.0",
    "tslib": "^1.10.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  }
}
